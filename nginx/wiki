server {
  listen 80 default_server;
  listen [::]:80 default_server;
  
  root /home/wiki/html;
  index index.html index.htm index.nginx-debian.html;
  
  server_name _;
  
  location / {
    try_files $uri $uri/ =404;
  }
  
  location /study/ {
    proxy_pass http://localhost:4567/study/;
  }
  
  location /temp/ {
    proxy_pass http://localhost:4568/temp/;
  }
}
